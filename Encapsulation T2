public class Person {
private String name;
  
public String getName(){
return name;
}
  
public void setName(String name){
this.name=name;
  }
}


public class Main {
    public static void main(String[] args){
    Person myObj=new Person();
    
    myObj.setName("John");
        System.out.println(myObj.getName());
    
    
    }
}
