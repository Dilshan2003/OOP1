public class Employee {
int empId;
String empName;
String gender;

Employee(int id,String name,String gen){
empId=id;
empName=name;
gender=gen;
}
 void display(){
     System.out.println("ID: "+empId+"\nName: "+empName+"\nGender: "+gender+"\n");
 }   
    public static void main(String[] args) {
        Employee emp1=new Employee(222,"Chethana","Female");
        Employee emp2=new Employee(223,"Nimesh","Male");
        emp1.display();
        emp2.display();
    }
    
}
